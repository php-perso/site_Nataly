<nav class="navbar navbar-expand-xl navbar-dark background_navbar">

    {# Logo petit écran #}
    <a href="{{ path('site_nataly_homepage') }}" id="logoPetitEcran">
        <img src="{{ asset('../../images/logo/na.png')}}" class="logo_nataly_navbar" alt="{% trans %}logo principal{% endtrans %}" />
    </a>

    {# Bouton Buger #}
    <button class="navbar-toggler custom-toggler" type="button" data-toggle="collapse" data-target="#navbar" aria-controls="navbar" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>

    {# Les Menus #}
    <div class="collapse navbar-collapse justify-content-md-center" id="navbar">

        {# Logo grand écran #}
        <a href="{{ path('site_nataly_homepage') }}" id="logoGrandEcran">
            <img src="{{ asset('../../images/logo/na.png')}}" class="logo_nataly_navbar" alt="{% trans %}logo principal{% endtrans %}" />
        </a>

        {# Liste de navigation #}
        <ul class="navbar-nav">
            
            {# Vers About me #}
            {% if menu == "about_me" %}
                <li class="police_nataly_navbar_select">
            {% else %}
                <li class="police_nataly_navbar">
            {% endif %}
                <a href="{{ path('site_nataly_about_me') }}">
                    About me
                </a>
            </li>

            {# Vers Portfolio #}
            {% if menu == "portfolio" %}
                <li class="police_nataly_navbar_select">
