                    Shop
                </a>
            </li>

            {# Vers Blog #}
            {% if menu == "blog" %}
                <li class="police_nataly_navbar_select">
            {% else %}
                <li class="police_nataly_navbar">
            {% endif %}
                <a href="{{ path('site_nataly_blog') }}">
                    Blog
                </a>
            </li>

            {# Vers Movie #}
            {% if menu == "movie" %}
                <li class="police_nataly_navbar_select">
            {% else %}
                <li class="police_nataly_navbar">
            {% endif %}
                <a href="{{ path('site_nataly_movie') }}">
                    Movie
                </a>
            </li>

            {# Vers Contact #}
            {% if menu == "contact" %}
